{% extends 'admin/layout.html.twig' %}

{% block title %}Dev{% endblock %}

{% block body %}
    <h1>Dev</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dev.id }}</td>
            </tr>
            <tr>
                <th>Fullname</th>
                <td>{{ dev.fullname }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ dev.email }}</td>
            </tr>
            <tr>
                <th>Devgroup_name</th>
                <td>{{ dev.devgroupName }}</td>
            </tr>
            
            <tr>
                <th>Gender</th>
                <td>{{ dev.gender }}</td>
            </tr>
            <tr>
                <th>Account_id</th>
                <td>{{ dev.accountId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_dev_index') }}">back to list</a>

    <a href="{{ path('app_dev_edit', {'id': dev.id}) }}">edit</a>

    {{ include('dev/_delete_form.html.twig') }}
